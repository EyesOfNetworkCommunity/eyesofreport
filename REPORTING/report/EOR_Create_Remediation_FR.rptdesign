            <xml-property name="queryText"><![CDATA[Select host_id, service_id, output_id 
from
(select host_id
from thruk.f6dd1_host where host_name in (select dho_name from d_host where dho_id = ?)) a,
(select output_id
from thruk.f6dd1_plugin_output 
where output ='OK;HARD;4;' limit 1) b,
(select case when nb = 0 then -1 else service_id end as service_id
from (select service_id, count(*) as nb
	  from thruk.f6dd1_service s inner join thruk.f6dd1_host h on h.host_id = s.host_id
	  where s.service_description in (select dse_name from d_service where dse_id = ?) and h.host_name in (select dho_name from d_host where dho_id = ?)) a) c]]></xml-property>
